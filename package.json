{
  "version": "3.0.0",
  "name": "shaneoneill.io",
  "description": "Simple starter for Gatsby",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy-s3": "gatsby-plugin-s3 deploy -y",
    "deploy": "npm run build && npm run deploy-s3",
    "prettier": "prettier --check '{,!(node_modules|assets)/**/}*.js*'"
  },
  "repository": "git+https://github.com/zeevosec/shaneoneill.io",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "theme",
    "starter"
  ],
  "author": "Shane O'Neill",
  "license": "MIT",
  "dependencies": {
    "gatsby": "^2.23.6",
    "gatsby-link": "^2.3.2",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-flow": "^1.2.1",
    "gatsby-plugin-google-analytics": "^2.3.10",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-optimize-svgs": "^1.0.4",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-copy-linked-files": "^2.3.10",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.4.10",
    "gatsby-remark-table-of-contents": "0.0.9",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.9.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
